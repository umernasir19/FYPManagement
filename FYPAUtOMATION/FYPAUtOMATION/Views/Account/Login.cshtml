    <script>
        $(document).ready(function(){


            //add customer button event
            $("#btnlogin").click(function (e) {
                e.preventDefault();
                var form = $("#loginform");

                $.validator.unobtrusive.parse(form);
                if ($(form).valid()) {
                    $.ajax({
                        type: "POST",
                        url: "/Account/Login",
                        data: form.serialize(),
                        //contentType: false,
                        //processData: false,
                        //cache: false,
                        success: function (res) {
                            console.log(res);
                            if (res.success) {
                                alert(res.msg);
                                window.location.href = res.url;
                            }
                            else if (res.success == false) {
                                alert(res.msg);
                                window.location.href = res.url;
                            }
                        },
                        failure: function (res) {
                            console.log(res);
                        },
                        err: function (res) {
                            console.log(res);
                        }

                    })
                }
                else {
                    return false;
                }

            });


        });
    </script>

